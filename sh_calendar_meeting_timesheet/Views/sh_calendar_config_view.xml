<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sh_calendar_config_view" model="ir.ui.view">
        <field name="name">sh_calendar_config_view</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="calendar.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//block[@name='calendar']" position="inside">
             
                    <setting id="sh_create_calendar" help="Automatic create timesheet directly">
                        <field name="group_create_calendar" />
                    </setting>

                
            </xpath>
        </field>
    </record>


   

   



</odoo>