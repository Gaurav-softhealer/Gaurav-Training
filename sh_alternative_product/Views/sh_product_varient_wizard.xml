<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="product_variant_wizard_view_form" model="ir.ui.view">
        <field name="name">product_variant.view.form</field>
        <field name="model">alternate.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="internal_ref" />
                        <!-- <field name="internal" /> -->
                        <field name="replacing_product_id" domain="temp"/>
                        <field name="temp" invisible="1"/>
                        <field name="alternate_products" readonly="1">
                            <!-- <list editable="bottom">
                                <field name="name" />
                                <field name="alternative_ids" widget="many2many_tags"/>
                            </list> -->
                        </field>   
                    </group>
                </sheet>
                    <footer>
                            <button name="replace_product" string="Replace" type="object" class="btn-primary" data-hotkey="q"/>
                            <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="x" />
                    </footer>
            </form>
        </field>
    </record>
</odoo>