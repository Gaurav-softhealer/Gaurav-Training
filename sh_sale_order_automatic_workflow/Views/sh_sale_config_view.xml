<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale_order_config_form" model="ir.ui.view">
        <field name="name">sale order config</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="sale.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//block[@id='connectors_setting_container']" position="after">
                <block title="Auto Sale Oder WorkFlow" id="auto_saleorder_config">
                    <setting id="auto_saleorder">
                        <field name="data" />
                    </setting>
                </block>
            </xpath>
        </field>
    </record>



</odoo>