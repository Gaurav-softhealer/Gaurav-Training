<?xml version='1.0' encoding='utf-8'?>
<odoo>

    <record id="demo_profile_view_tree" model="ir.ui.view">
        <field name="name">demo_profile.view.tree</field>
        <field name="model">demo.profile</field>
        <field name="arch" type="xml">
            <list string="">
                <field name="name" />
                    <field name="age" />
                    <field name="gender" />
                    <field name="image" widget="image" />
                    <field name="user_id" />
            </list>
        </field>
    </record>

    <record id="demo_profile_view_form" model="ir.ui.view">
        <field name="name">demo_profile.view.form</field>
        <field name="model">demo.profile</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="age" />
                        <field name="gender" />
                        <field name="image" widget="image" />
                        <field name="user_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="demo_profile_action" model="ir.actions.act_window">
        <field name="name">Demo profile</field>
        <field name="res_model">demo.profile</field>
        <field name="view_mode">list,form</field>

    </record>

    <menuitem
        id="demo_data_menu"
        name="Demo"
        sequence="10"/>
    <menuitem
        id="demo_data_profiole_menu"
        name="Profile"
        sequence="10"
        parent="sh_demo_data.demo_data_menu"
        action="demo_profile_action"/>
</odoo>