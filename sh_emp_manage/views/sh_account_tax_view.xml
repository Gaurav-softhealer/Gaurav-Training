<?xml version="1.0" encoding="utf-8"?>

<odoo>

<!-- view -->

      <!-- list -->

      <record model="ir.ui.view" id="sh_account_list">
            <field name="name">Account list view</field>
            <field name="model">sh.account.tax</field>
            <field name="arch" type="xml">
            <list>
             
                <field name="name" />
            


            </list>    
            </field>
        </record>

     <!-- form view -->

     <record model="ir.ui.view" id="sh_account_form">
            <field name="name">Account form view</field>
            <field name="model">sh.account.tax</field>
            <field name="arch" type="xml">
            <form>
              <sheet>
              <group>
                <field name="name" required="1"/>
                <field name="tax"/>
                


                </group>
              </sheet>
            </form>    
            </field>
        </record>


        <!-- karban view -->




        <!-- search view -->



<!-- actions -->



    <record id="sh_account_action" model="ir.actions.act_window">
        <field name="name">Account Tax list</field>
        <field name="res_model">sh.account.tax</field>
        <field name="view_mode">list,form,kanban</field>
    </record>



   

<!-- manu -->
    

</odoo>
