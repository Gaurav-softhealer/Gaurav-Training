<?xml version="1.0" encoding="utf-8"?>

<odoo>

<!-- view -->

      <!-- list -->

      <record model="ir.ui.view" id="sh_partner_list">
            <field name="name">partner list view</field>
            <field name="model">sh.res.partner</field>
            <field name="arch" type="xml">
            <list>
             
                <field name="name" />
                <field name="city"/>


            </list>    
            </field>
        </record>

     <!-- form view -->

     <record model="ir.ui.view" id="sh_partner_form">
            <field name="name">partner form view</field>
            <field name="model">sh.res.partner</field>
            <field name="arch" type="xml">
            <form>
              <sheet>
              <group>
                <field name="name" required="1"/>
                <field name="city"/>


                </group>
              </sheet>
            </form>    
            </field>
        </record>


        <!-- karban view -->




        <!-- search view -->



<!-- actions -->



    <record id="sh_partner_action" model="ir.actions.act_window">
        <field name="name">Partner list</field>
        <field name="res_model">sh.res.partner</field>
        <field name="view_mode">list,form,kanban</field>
    </record>



   

<!-- manu -->
    

</odoo>
