<?xml version="1.0" encoding="utf-8"?>

<odoo>

<!-- view -->

      <!-- list -->

      <record model="ir.ui.view" id="sh_orm_create_list">
            <field name="name">Orm create list view</field>
            <field name="model">sh.orm.create</field>
            <field name="arch" type="xml">
            <list>
             
                <field name="stu_name" />
                <field name="stu_age" />


            </list>    
            </field>
        </record>

     <!-- form view -->

     <record model="ir.ui.view" id="sh_orm_create_form">
            <field name="name">doctor form view</field>
            <field name="model">sh.orm.create</field>
            <field name="arch" type="xml">
            <form>
              <sheet>
              <group>
                <field name="stu_no" readonly="1"/>
                <field name="stu_name" required="1"/>
                <field name="stu_age"/>
                <field name="price"/>
                <field name="gender"/>
                <field name="dep_id"/>
                <field name="parent"/>
                <field name="cat_ids" widget="many2many_tags"/>



                </group>
              </sheet>
            </form>    
            </field>
        </record>


        <!-- karban view -->




        <!-- search view -->



<!-- actions -->



    <record id="sh_orm_create_action" model="ir.actions.act_window">
        <field name="name">ORM create list</field>
        <field name="res_model">sh.orm.create</field>
        <field name="view_mode">list,form,kanban</field>
    </record>



   

<!-- manu -->



</odoo>
