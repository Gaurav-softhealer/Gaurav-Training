<?xml version="1.0" encoding="utf-8"?>

<odoo>

<!-- view -->

      <!-- list -->

      <record model="ir.ui.view" id="sh_age_category_list">
            <field name="name">age category list view</field>
            <field name="model">sh.age.category</field>
            <field name="arch" type="xml">
            <list>
             
                <field name="name" />



            </list>    
            </field>
        </record>

     <!-- form view -->

     <record model="ir.ui.view" id="sh_age_category_form">
            <field name="name">age category form view</field>
            <field name="model">sh.age.category</field>
            <field name="arch" type="xml">
            <form>
              <sheet>
              <group>
                <field name="name" required="1"/>
                <field name="min_age" />
                <field name="max_age" />


                </group>
              </sheet>
            </form>    
            </field>
        </record>


        <!-- karban view -->




        <!-- search view -->



<!-- actions -->



    <record id="sh_age_category_action" model="ir.actions.act_window">
        <field name="name">age category list</field>
        <field name="res_model">sh.age.category</field>
        <field name="view_mode">list,form,kanban</field>
    </record>



   

<!-- manu -->

<menuitem
            name="AgeCategory"
            id="age_category_menu"
            sequence="4"
            action="sh_age_category_action"
            parent="sh_emp_manage.hospital_menu"
        />
    

</odoo>
