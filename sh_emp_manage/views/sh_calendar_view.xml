<?xml version="1.0" encoding="utf-8"?>

<odoo>

<!-- view -->

      <!-- list -->

      <record model="ir.ui.view" id="sh_dep_list3">
            <field name="name">Job list view2</field>
            <field name="model">sh.calendar</field>
            <field name="arch" type="xml">
            <list>
             
                <field name="name" />
                <field name="status"/>
               

            </list>    
            </field>
        </record>

     <!-- form view -->

     <record model="ir.ui.view" id="sh_cal_list3">
            <field name="name">Job list view2</field>
            <field name="model">sh.calender</field>
            <field name="arch" type="xml">
            <form>
              <sheet>
              <group>
                <field name="name" />
                <field name="parent_dep_id" options="{'no_create':1}"/>
                <field name="manager_id" options="{'no_create':1}"/>
                <!-- <field name="job_ids" options="{'no_create':1}"/> -->
                </group>
              </sheet>
            </form>    
            </field>
        </record>


        <!-- karban view -->




        <!-- search view -->



<!-- actions -->

    <record id="sh_emp_action4" model="ir.actions.act_window">
        <field name="name">calendar list</field>
        <field name="res_model">sh.calendar</field>
        <field name="view_mode">list,form,kanban</field>
        <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
            No product found. Let's create one!
          </p><p>
            You must define a product for everything you sell or purchase,
            whether it's a storable product, a consumable or a service.
          </p>
        </field>
    </record>



   

<!-- manu -->

    <menuitem
        name="Calender"
        id="calender_menu"
        parent="sh_emp_manage.emp_manage_menu"
        sequence="5"
        action="sh_emp_action4"
    />    

</odoo>
